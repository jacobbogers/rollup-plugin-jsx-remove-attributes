{
	"$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
	"organizeImports": {
		"ignore": [
			"node_modules",
			"dist"
		],
		"enabled": true
	},
	"formatter": {
		"ignore": [
			"node_modules",
			"dist"
		],
		"lineEnding": "lf",
		"indentWidth": 4
	},
	"linter": {
		"enabled": true,
		"ignore": [
			"node_modules",
			"dist"
		],
		"rules": {
			"recommended": true
		}
	}
}